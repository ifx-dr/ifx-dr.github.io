<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.w3.org/2002/07/owl#"
     xml:base="http://www.w3.org/2002/07/owl"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#">
    <Ontology/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/2004/02/skos/core#altLabel -->


    <AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#altLabel"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3id.org/ecsel-dr-PMV#activates -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#activates">
        <inverseOf rdf:resource="http://www.w3id.org/ecsel-dr-PMV#is_activated_by"/>
        <rdfs:domain rdf:resource="http://org.semanticweb.owlapi/error#Error353"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Modelling_Function_Object"/>
        <rdfs:comment>The connector is used to represent the chronological order in an EPC, where an Event or a Rule activate a Task or Process Interface. </rdfs:comment>
        <rdfs:label xml:lang="en">activates</rdfs:label>
        <skos:altLabel>activ</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#activates_function -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#activates_function">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/ecsel-dr-PMV#activates"/>
        <inverseOf rdf:resource="http://www.w3id.org/ecsel-dr-PMV#is_activated_by_function"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Event"/>
        <rdfs:range rdf:resource="http://org.semanticweb.owlapi/error#Error345"/>
        <rdfs:comment>Specialization of the activates property that links the class Event with Task and Process Interface in the course of the process flow.</rdfs:comment>
        <rdfs:label xml:lang="en">activates function</rdfs:label>
        <skos:altLabel>activ function</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#activates_task -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#activates_task">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/ecsel-dr-PMV#activates"/>
        <inverseOf rdf:resource="http://www.w3id.org/ecsel-dr-PMV#is_activated_by_task"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Rule"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Task"/>
        <rdfs:comment>Specialization of the activates property that links the class Rule with Task in the course of the process flow.</rdfs:comment>
        <rdfs:label xml:lang="en">activates task</rdfs:label>
        <skos:altLabel>activ task</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#belongs_to_model -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#belongs_to_model">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Model_Element"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Model"/>
        <rdfs:comment>Denotes the connection between a &quot;Function&quot; and another &quot;Process&quot; as this process may deliver an output used in this function.</rdfs:comment>
        <rdfs:label xml:lang="en">belongs to model</rdfs:label>
        <skos:altLabel>belong</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#carries_out_task -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#carries_out_task">
        <inverseOf rdf:resource="http://www.w3id.org/ecsel-dr-PMV#is_carried_out_by"/>
        <rdfs:domain rdf:resource="http://org.semanticweb.owlapi/error#Error363"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Role"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Task"/>
        <rdfs:comment>Role(s) or Organizational unit(s) that carrries (carry) out the process step (equivalent to R in RACI notation)</rdfs:comment>
        <rdfs:label xml:lang="en">carries out task</rdfs:label>
        <skos:altLabel>carri</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#connected_to -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#connected_to">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Model"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Model"/>
        <rdfs:comment xml:lang="en">Represents the connections and dependencies between models</rdfs:comment>
        <rdfs:label xml:lang="en">connected to</rdfs:label>
        <skos:altLabel>connect</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#contains -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#contains">
        <inverseOf rdf:resource="http://www.w3id.org/ecsel-dr-PMV#diagramed_on_model"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Model"/>
        <rdfs:range rdf:resource="http://org.semanticweb.owlapi/error#Error344"/>
        <rdfs:comment xml:lang="en">Links a Model with the elements represented by it.</rdfs:comment>
        <rdfs:label xml:lang="en">contains</rdfs:label>
        <skos:altLabel>contain</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#contributes_to_task -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#contributes_to_task">
        <inverseOf rdf:resource="http://www.w3id.org/ecsel-dr-PMV#is_worked_on_by_collaboration_of"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Role"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Task"/>
        <rdfs:comment>Role(s) that contribute(s) to a process step during it is carried out.</rdfs:comment>
        <rdfs:label xml:lang="en">contributes to task</rdfs:label>
        <skos:altLabel>contribut</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#controls_function -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#controls_function">
        <inverseOf rdf:resource="http://www.w3id.org/ecsel-dr-PMV#is_controlled_by_goal"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Goal"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Function"/>
        <rdfs:comment>A goal controls the outcome of a function in a process flow.</rdfs:comment>
        <rdfs:label xml:lang="en">controls function</rdfs:label>
        <skos:altLabel>control</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#creates_event -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#creates_event">
        <inverseOf rdf:resource="http://www.w3id.org/ecsel-dr-PMV#is_created_by"/>
        <rdfs:domain rdf:resource="http://org.semanticweb.owlapi/error#Error350"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Modelling_Function_Object"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Event"/>
        <rdfs:comment>A process step creates an event. The connector is used to represent the chronological order in an EPC.</rdfs:comment>
        <rdfs:label xml:lang="en">creates event</rdfs:label>
        <skos:altLabel>creat</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#diagramed_on_model -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#diagramed_on_model">
        <rdfs:domain rdf:resource="http://org.semanticweb.owlapi/error#Error349"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Model"/>
        <rdfs:comment xml:lang="en">Links a FuncitonObject or a ModelElement with the Model that it&apos;s represented in.</rdfs:comment>
        <rdfs:label xml:lang="en">diagramed on model</rdfs:label>
        <skos:altLabel>diagram</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#evaluates -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#evaluates">
        <inverseOf rdf:resource="http://www.w3id.org/ecsel-dr-PMV#is_evaluated_by"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Join_Rule"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Event"/>
        <rdfs:comment>A rule evaluates an event. The connector is used to represent the chronological order in an EPC.</rdfs:comment>
        <rdfs:label xml:lang="en">evaluates</rdfs:label>
        <skos:altLabel>evalu</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#executes_software -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#executes_software">
        <inverseOf rdf:resource="http://www.w3id.org/ecsel-dr-PMV#is_executed_by_function"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Function"/>
        <rdfs:range rdf:resource="http://purl.org/dc/dcmitype/Software"/>
        <rdfs:comment>A function in a process flow excutes software.</rdfs:comment>
        <rdfs:label xml:lang="en">executes software</rdfs:label>
        <skos:altLabel>execut</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#flow -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#flow">
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Model_Element"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Model_Element"/>
        <rdfs:comment>Enables the modelling of the process flow by logically connecting the different process elements. Therefore, the domain and the range are set to a &quot;Process Element&quot;.</rdfs:comment>
        <rdfs:label xml:lang="en">flow</rdfs:label>
        <skos:altLabel>flow</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#follows -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#follows">
        <inverseOf rdf:resource="http://www.w3id.org/ecsel-dr-PMV#is_predecessor_of"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Modelling_Function_Object"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Task"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Modelling_Function_Object"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Task"/>
        <rdfs:comment>A process step follows a process step. The connector is used to represent the chronological order in an EPC.</rdfs:comment>
        <rdfs:label xml:lang="en">follows</rdfs:label>
        <skos:altLabel>follow</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#has_as_input -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#has_as_input">
        <inverseOf rdf:resource="http://www.w3id.org/ecsel-dr-PMV#is_input_for"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Task"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Technical_Term"/>
        <rdfs:comment>A process step can have technical term(s) as input.</rdfs:comment>
        <rdfs:label xml:lang="en">has as input</rdfs:label>
        <skos:altLabel>input</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#has_as_output -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#has_as_output">
        <inverseOf rdf:resource="http://www.w3id.org/ecsel-dr-PMV#is_output_of"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Function"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Task"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Output"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Technical_Term"/>
        <rdfs:comment>A function in a process flow produces a physical or non-physical output.</rdfs:comment>
        <rdfs:comment>A process step can have technical term(s) as output.</rdfs:comment>
        <rdfs:label xml:lang="en">has as output</rdfs:label>
        <skos:altLabel>output</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#has_next_model -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#has_next_model">
        <inverseOf rdf:resource="http://www.w3id.org/ecsel-dr-PMV#has_previous_model"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Business_Process_Flow"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Business_Process_Flow"/>
        <rdfs:comment>An EPC can have another EPC following which is dependent on the result. The connector is used to represent the chronological order in a VACD.</rdfs:comment>
        <rdfs:label>has next model</rdfs:label>
        <skos:altLabel>model</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#has_object -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#has_object">
        <inverseOf rdf:resource="http://www.w3id.org/ecsel-dr-PMV#relevant_to"/>
        <rdfs:domain rdf:resource="http://org.semanticweb.owlapi/error#Error352"/>
        <rdfs:range rdf:resource="http://org.semanticweb.owlapi/error#Error339"/>
        <rdfs:comment>EPCs and Value-added chain diagrams consist of objects.</rdfs:comment>
        <rdfs:label xml:lang="en">has object</rdfs:label>
        <skos:altLabel>object</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#has_previous_model -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#has_previous_model">
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Business_Process_Flow"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Business_Process_Flow"/>
        <rdfs:comment>An EPC can have another EPC previous to itself and is dependent on its results. The connector is used to represent the chronological order in a VACD.</rdfs:comment>
        <rdfs:label xml:lang="en">has previous model</rdfs:label>
        <skos:altLabel>previou model</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#has_sub_flow -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#has_sub_flow">
        <inverseOf rdf:resource="http://www.w3id.org/ecsel-dr-PMV#is_sub_flow_of"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Task"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Business_Process_Flow"/>
        <rdfs:comment>EPCs can have other EPCs as subordinates. The connector is used to represent the hierarchical order.</rdfs:comment>
        <rdfs:label xml:lang="en">has sub flow</rdfs:label>
        <skos:altLabel>sub flow</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#has_sub_model -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#has_sub_model">
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Function"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Model"/>
        <rdfs:comment>Denotes to which instance of &quot;Process&quot; an instance of a &quot;Process Element&quot; belongs.</rdfs:comment>
        <rdfs:label xml:lang="en">has sub model</rdfs:label>
        <skos:altLabel>sub model</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#has_subordinate -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#has_subordinate">
        <inverseOf rdf:resource="http://www.w3id.org/ecsel-dr-PMV#has_superior"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Value_Added_Chain_Diagram"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Business_Process_Flow"/>
        <rdfs:comment>Value-added chain diagrams have EPCs as subordinates. The connector is used to represent the hierarchical order.</rdfs:comment>
        <rdfs:label>has subordinate</rdfs:label>
        <skos:altLabel>subordin</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#has_subordinate_VACD -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#has_subordinate_VACD">
        <inverseOf rdf:resource="http://www.w3id.org/ecsel-dr-PMV#has_superior_VACD"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Value_Added_Chain_Diagram"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Value_Added_Chain_Diagram"/>
        <rdfs:comment>Value-added chain diagrams have value-added chain diagrams as subordinates. The connector is used to represent the hierarchical order.</rdfs:comment>
        <rdfs:label>has subordinate VACD</rdfs:label>
        <skos:altLabel>subordin vacd</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#has_superior -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#has_superior">
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Business_Process_Flow"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Value_Added_Chain_Diagram"/>
        <rdfs:comment>EPCs have value-added chain diagrams as superior. The connector is used to represent the hierarchical order.</rdfs:comment>
        <rdfs:label>has superior</rdfs:label>
        <skos:altLabel>superior</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#has_superior_VACD -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#has_superior_VACD">
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Value_Added_Chain_Diagram"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Value_Added_Chain_Diagram"/>
        <rdfs:comment>Value-added chain diagrams have another value-added chain diagram as superior. The connector is used to represent the hierarchical order.</rdfs:comment>
        <rdfs:label>has superior VACD</rdfs:label>
        <skos:altLabel>superior vacd</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#is_activated_by -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#is_activated_by">
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Modelling_Function_Object"/>
        <rdfs:range rdf:resource="http://org.semanticweb.owlapi/error#Error338"/>
        <rdfs:comment>A process step is activated by an event or a rule. The connector is used to represent the chronological order in an EPC.</rdfs:comment>
        <rdfs:label>is activated by</rdfs:label>
        <skos:altLabel>activ</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#is_activated_by_function -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#is_activated_by_function">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/ecsel-dr-PMV#is_activated_by"/>
        <rdfs:domain rdf:resource="http://org.semanticweb.owlapi/error#Error346"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Event"/>
        <rdfs:comment>Represents the inverse property of activatesFunction.</rdfs:comment>
        <rdfs:label>is activated by function</rdfs:label>
        <skos:altLabel>activ function</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#is_activated_by_task -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#is_activated_by_task">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/ecsel-dr-PMV#is_activated_by"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Task"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Rule"/>
        <rdfs:comment>Represents the inverse property of activatesTask.</rdfs:comment>
        <rdfs:label>is activated by task</rdfs:label>
        <skos:altLabel>activ task</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#is_assigned_to -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#is_assigned_to">
        <inverseOf rdf:resource="http://www.w3id.org/ecsel-dr-PMV#leads_to_rule"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Rule"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Modelling_Function_Object"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-PMV#Task"/>
        <rdfs:comment>A rule is assigned to a process step. The connector is used to represent the chronological order in an EPC.</rdfs:comment>
        <rdfs:label xml:lang="en">is assigned to</rdfs:label>
        <skos:altLabel>assign</skos:altLabel>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-PMV#is_carried_out_by -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-PMV#is_carried_out_by">
