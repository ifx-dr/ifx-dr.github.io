                       :hasQuantity 10 .


###  http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Capacity_Restriction_Equation
:Capacity_Restriction_Equation rdf:type owl:NamedIndividual ,
                                        :PlanningConstraint ;
                               :sc3:formula """forall($k in WorkCenters, $j in Frontends, t in PlanningHorizon) sum($g in Products, $l in OperationCountOnWorkCenter($j, $k, $g) ->
		FE_Y($j, $g, $t, OperationsOnWorkCenter($j, $k, $g, $l)) * alpha($j, $g, OperationsOnWorkCenter($j, $k, $g, $l)) <= WorkCenterCapacity($t, $j, $k);
	

WorkCenters (Resources)=FE_Aligner, FE_Stepper_2, FE_Stepper_1, FE_Implanter
Frontends= Frontend_1, Frontend_2;
Products= Product_1;
OperationCountOnWorkCenter=Product($numberOfOperationsOnSameResource);
alpha= Activity($hasRequiredCapacity(Product($hasActivities)));
WorkCenterCapacity= FE_Aligner($hasAvailableCapacity, $hasLocation), FE_Stepper_2($hasAvailableCapacity, $hasLocation), FE_Stepper_1($hasAvailableCapacity, $hasLocation), FE_Implanter($hasAvailableCapacity, $hasLocation);""" .


###  http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#FE1
:FE1 rdf:type owl:NamedIndividual ,
              :Owner .


###  http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#FE1_CostMinimization
:FE1_CostMinimization rdf:type owl:NamedIndividual ,
                               :PlaningPurpose ;
                      :hasCriterias :Minimization ;
                      :hasPerformanceMeasures :MinimizeBacklog ,
                                              :MinimizeInventoryLevel ,
                                              :MinimizeWipCosts ;
                      :hasPlanningObjects :FE_Backlog ,
                                          :FE_Inventory ,
                                          :FE_Wip ,
                                          :FE_X ,
                                          :FE_Y ;
                      :hasCosts "backlog=150" ,
                                "inventory=15" ,
                                "wip=50" ;
                      :hasPlanningWindow "12 weeks" ;
                      :sc3:formula """sum($g in Products, $t in PlanningHorizon)--> sum($j in Frontends) (WipCosts($g, $t) * FE_Wip($j, $g, $t)) + InventoryCosts($g, $t) * FE_Inventory($g, $t) + BacklogCosts($g, $t) * FE_Backlog($g, $t);

Frontends= Frontend_1, Frontend_2;
Products= Product_1;
InventoryCosts= $(hasCosts($inventory));
BacklogCosts= $(hasCosts($backlog));
WipCosts= $(hasCosts($wip));""" .


