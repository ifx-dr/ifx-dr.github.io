    <!-- http://www.w3id.org/ecsel-dr-Planning#FE1_CostMinimization -->


    <NamedIndividual rdf:about="http://www.w3id.org/ecsel-dr-Planning#FE1_CostMinimization">
        <rdf:type rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Planing_Purpose"/>
        <ecsel-dr-Planning:has_criterias rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Minimization"/>
        <ecsel-dr-Planning:has_performance_measures rdf:resource="http://www.w3id.org/ecsel-dr-Planning#MinimizeBacklog"/>
        <ecsel-dr-Planning:has_performance_measures rdf:resource="http://www.w3id.org/ecsel-dr-Planning#MinimizeInventoryLevel"/>
        <ecsel-dr-Planning:has_performance_measures rdf:resource="http://www.w3id.org/ecsel-dr-Planning#MinimizeWipCosts"/>
        <ecsel-dr-Planning:has_planning_objects rdf:resource="http://www.w3id.org/ecsel-dr-Planning#FE_Backlog"/>
        <ecsel-dr-Planning:has_planning_objects rdf:resource="http://www.w3id.org/ecsel-dr-Planning#FE_Inventory"/>
        <ecsel-dr-Planning:has_planning_objects rdf:resource="http://www.w3id.org/ecsel-dr-Planning#FE_Wip"/>
        <ecsel-dr-Planning:has_planning_objects rdf:resource="http://www.w3id.org/ecsel-dr-Planning#FE_X"/>
        <ecsel-dr-Planning:has_planning_objects rdf:resource="http://www.w3id.org/ecsel-dr-Planning#FE_Y"/>
        <ecsel-dr-Planning:has_costs>backlog=150</ecsel-dr-Planning:has_costs>
        <ecsel-dr-Planning:has_costs>inventory=15</ecsel-dr-Planning:has_costs>
        <ecsel-dr-Planning:has_costs>wip=50</ecsel-dr-Planning:has_costs>
        <ecsel-dr-Planning:has_planning_window>12 weeks</ecsel-dr-Planning:has_planning_window>
        <sc3:formula>sum($g in Products, $t in PlanningHorizon)--&gt; sum($j in Frontends) (WipCosts($g, $t) * FE_Wip($j, $g, $t)) + InventoryCosts($g, $t) * FE_Inventory($g, $t) + BacklogCosts($g, $t) * FE_Backlog($g, $t);

Frontends= Frontend_1, Frontend_2;
Products= Product_1;
InventoryCosts= $(hasCosts($inventory));
BacklogCosts= $(hasCosts($backlog));
WipCosts= $(hasCosts($wip));</sc3:formula>
    </NamedIndividual>
    


