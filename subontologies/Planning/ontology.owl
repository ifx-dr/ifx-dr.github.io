     -->


    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_DMUa -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_DMUa">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Vertex"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Decision_Making_Unit"/>
        <rdfs:comment>First DMU of the Vertex</rdfs:comment>
        <rdfs:label xml:lang="en">has DMUa</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_DMUb -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_DMUb">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Edge"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Decision_Making_Unit"/>
        <rdfs:comment>Second DMU of the Vertex</rdfs:comment>
        <rdfs:label xml:lang="en">has DMUb</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_activities -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_activities">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Planning_Object"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Product"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Required_Capacity"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Activity"/>
        <rdfs:comment>This property describes the set of activities that must be carried out to produce the goods that are associated with the PLANNING OBJECT.

Moreover, there are additional parameters for most types of PLANNING OBJECTS.</rdfs:comment>
        <rdfs:label xml:lang="en">has activities</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_bill_of_material -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_bill_of_material">
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Part"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Product"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Bill_Of_Material"/>
        <rdfs:comment>A set of potentially alternative BOMs can be used to meet the demand of a certain product</rdfs:comment>
        <rdfs:label xml:lang="en">has bill of material</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_bin -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_bin">
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Binning"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Bin"/>
        <rdfs:label xml:lang="en">has bin</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_capacity -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_capacity">
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Process"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Resource"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Available_Capacity"/>
        <rdfs:label xml:lang="en">has capacity</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_child_product_aggregate -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_child_product_aggregate">
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Product_Hierarchy"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Product_Hierarchy"/>
        <rdfs:label xml:lang="en">has child product aggregate</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_component_location -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_component_location">
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Bill_Of_Material"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Location"/>
        <rdfs:label xml:lang="en">has component location</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_component_part -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_component_part">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Bill_Of_Material"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Process"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Component_Part"/>
        <rdfs:label xml:lang="en">has component part</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_component_process -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_component_process">
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Bill_Of_Material"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Process"/>
        <rdfs:label xml:lang="en">has component process</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_connection -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_connection">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Decision_Making_Unit"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Planning_Process"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Edge"/>
        <rdfs:comment>If a vertex provides instruction or a reaction to another vertex than the two vertices are connected by a directed edge.</rdfs:comment>
        <rdfs:label xml:lang="en">has connection</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_constraint -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_constraint">
        <rdfs:label xml:lang="en">has constraint</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_criterias -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_criterias">
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Planing_Purpose"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Criteria"/>
        <rdfs:comment>A criterion is assigned to each performance measure that belongs to a PLANNING PURPOSE. Typical criteria are maximization, minimization, i.e. if an optimization problem is considered, or larger or smaller than a prescribed value of the certain performance measure, a so-called target, i.e. if a satisfaction problem is solved. In a multi-criteria setting, the dominance concept for plans is of interest.</rdfs:comment>
        <rdfs:label xml:lang="en">has criterias</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_decision_maker_type -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_decision_maker_type">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Decision_Making_Unit"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Decision_Maker_Type"/>
        <rdfs:comment>Specifies the concrete type of a DMU</rdfs:comment>
        <rdfs:label xml:lang="en">has decision maker type</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_decision_making_initiative -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_decision_making_initiative">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Decision_Making_Unit"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Decision_Making_Initiative"/>
        <rdfs:comment>We distinguish fully autonomous DECISION-MAKING UNITS from DECISION-MAKING UNITS where the decision-making activities are triggered by other DECISION-MAKING UNITS. Note that the decision-making initiative can change over time in a situation-dependent manner.</rdfs:comment>
        <rdfs:label xml:lang="en">has decision making initiative</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_decision_making_unit -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_decision_making_unit">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Planning_Situation"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Vertex"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Decision_Making_Unit"/>
        <rdfs:comment>This property describes the set of DECISION-MAKING UNITS that constitutes the planning situation. We refer to the elements of the set as vertices.</rdfs:comment>
        <rdfs:label xml:lang="en">has decision making unit</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_decision_making_unit_charcters -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_decision_making_unit_charcters">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Planning_Situation"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Decison_Making_Unit_Character"/>
        <rdfs:label xml:lang="en">has decision making unit charcters</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_defined -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_defined">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Plan_Execution"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Performance_Measure"/>
        <rdfs:comment>Values for certain PERFORMANCE MEASURES can be computed after a plan is executed. This always requires that the consequences of a PLAN are assessed in the base system.</rdfs:comment>
        <rdfs:label xml:lang="en">has defined</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_event_trigger -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_event_trigger">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Planning_Process"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Trigger"/>
        <rdfs:comment>This property determines which events result in replanning (Vieria et al. 2003). For instance, replanning can be the result of the breakdown of a bottleneck resource.</rdfs:comment>
        <rdfs:label xml:lang="en">has event trigger</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_frozen_fence -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_frozen_fence">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Planning_Process"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Planning_Decision"/>
        <rdfs:comment>This property specifies which planning decisions are remain unchanged in the case of replanning activities.</rdfs:comment>
        <rdfs:label xml:lang="en">has frozen fence</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_hierarchical_situation -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_hierarchical_situation">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Planning_Situation"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Hierarchical_Situation"/>
        <rdfs:label xml:lang="en">has hierarchical situation</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_involved_planning_objects -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_involved_planning_objects">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Planning_Instance"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Planning_Object"/>
        <rdfs:comment>This property specifies the involved PLANNING OBJECTS.</rdfs:comment>
        <rdfs:label xml:lang="en">has involved planning objects</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_location -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_location">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Bill_Of_Material"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Binning"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Capacity"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Frozen_Fence"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Inventory"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Inventory_Policy"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Lot_Sizing_Rule"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Planning_Object"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Process"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Product"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Projected_WIP"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Substitution"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Supply_Information"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Yield"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Location"/>
        <rdfs:comment>This property describes the place or places, for instance nodes of the semiconductor supply chain or specific machines of a work center where the ACTIVITIES associated with the PLANNING OBJECT have to be executed.</rdfs:comment>
        <rdfs:label xml:lang="en">has location</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_objectives -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_objectives">
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Planning_Process"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Planing_Purpose"/>
        <rdfs:comment>: The property specifies the objectives of a certain planning activity.
Moreover, there are additional parameters for most types of PLANNING PROCESS.</rdfs:comment>
        <rdfs:label xml:lang="en">has objectives</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_operations -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_operations">
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Route"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Macro_Operation"/>
        <rdfs:label xml:lang="en">has operations</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_order -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_order">
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#In_Transit_Inventory"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Order"/>
        <rdfs:label xml:lang="en">has order</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_owner -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_owner">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Planning_Object"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Owner"/>
        <rdfs:comment>A PLANNING OBJECT is rarely considered in isolation. Usually it belongs to a set of PLANNING OBJECTS, for example, a planning instance.</rdfs:comment>
        <rdfs:label xml:lang="en">has owner</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_parent_product_aggregate -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_parent_product_aggregate">
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Product_Hierarchy"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Product_Hierarchy"/>
        <rdfs:label xml:lang="en">has parent product aggregate</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_part -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_part">
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Bin"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Binning"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Projected_WIP"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Yield"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Part"/>
        <rdfs:label xml:lang="en">has part</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_performance_measures -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_performance_measures">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Planing_Purpose"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Performance_Measure"/>
        <rdfs:comment>Each PLANNING PURPOSE is determined by a set of performance measures. Each measure is defined by a calculation procedure. Performance measures can be blended using weights to express the importance of the different measures.</rdfs:comment>
        <rdfs:label xml:lang="en">has performance measures</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_periodic_trigger -->


    <ObjectProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_periodic_trigger">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Planning_Process"/>
        <rdfs:range rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Trigger"/>
        <rdfs:comment>This property defines after which amount of time a replanning activity is initiated in a periodic manner (Vieria et al. 2003). Rolling horizon approaches are triggered by such a periodic approach.</rdfs:comment>
        <rdfs:label xml:lang="en">has periodic trigger</rdfs:label>
