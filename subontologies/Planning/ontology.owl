            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Year duration</rdfs:comment>
        <rdfs:label xml:lang="en">Year</rdfs:label>
        <owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:deprecated>
