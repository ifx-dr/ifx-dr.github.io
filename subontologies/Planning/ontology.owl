    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasFeasibilityStatus -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasFeasibilityStatus">
