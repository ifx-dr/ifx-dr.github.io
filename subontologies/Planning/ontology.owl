        <rdf:type rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Activity"/>
        <ecsel-dr-Planning:has_capacity rdf:resource="http://www.w3id.org/ecsel-dr-Planning#RequiredCap4"/>
        <ecsel-dr-Planning:has_resources rdf:resource="http://www.w3id.org/ecsel-dr-Planning#FE_Stepper_2"/>
    </NamedIndividual>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#All_Planning_Objects_have_to_be_positive -->


    <NamedIndividual rdf:about="http://www.w3id.org/ecsel-dr-Planning#All_Planning_Objects_have_to_be_positive">
        <rdf:type rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Planning_Constraint"/>
        <sc3:formula>forall($g in Products, $t in PlanningHorizon, $j in Frontends) -&gt; FE_Y($j, $g, $t) &gt;= 0;
forall($g in Products, $t in PlanningHorizon, $j in Frontends) -&gt; FE_X($j, $g, $t) &gt;= 0;
forall($g in Products, $t in PlanningHorizon, $j in Frontends) -&gt; FE_Wip($j, $g, $t) &gt;= 0;
forall($g in Products, $t in PlanningHorizon) -&gt; FE_Inventory($g, $t) &gt;= 0;
forall($g in Products, $t in PlanningHorizon) -&gt; FE_Backlog($g, $t) &gt;= 0;</sc3:formula>
    </NamedIndividual>
    


