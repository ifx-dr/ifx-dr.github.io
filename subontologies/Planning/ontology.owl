    <NamedIndividual rdf:about="http://www.w3id.org/ecsel-dr-Planning#Borderless_FE1_zu_FE2">
        <rdf:type rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Planning_Object"/>
        <ecsel-dr-Planning:has_activities rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Activity_Photo"/>
        <ecsel-dr-Planning:has_location rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Frontend_2"/>
        <ecsel-dr-Planning:has_owner rdf:resource="http://www.w3id.org/ecsel-dr-Planning#FE1"/>
        <ecsel-dr-Planning:has_product rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Product_1"/>
        <ecsel-dr-Planning:due_date>1 week</ecsel-dr-Planning:due_date>
        <ecsel-dr-Planning:quantity rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</ecsel-dr-Planning:quantity>
    </NamedIndividual>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#Capacity_Restriction_Equation -->


    <NamedIndividual rdf:about="http://www.w3id.org/ecsel-dr-Planning#Capacity_Restriction_Equation">
        <rdf:type rdf:resource="http://www.w3id.org/ecsel-dr-Planning#Planning_Constraint"/>
        <sc3:formula>forall($k in WorkCenters, $j in Frontends, t in PlanningHorizon) sum($g in Products, $l in OperationCountOnWorkCenter($j, $k, $g) -&gt;
		FE_Y($j, $g, $t, OperationsOnWorkCenter($j, $k, $g, $l)) * alpha($j, $g, OperationsOnWorkCenter($j, $k, $g, $l)) &lt;= WorkCenterCapacity($t, $j, $k);


WorkCenters (Resources)=FE_Aligner, FE_Stepper_2, FE_Stepper_1, FE_Implanter
Frontends= Frontend_1, Frontend_2;
Products= Product_1;
OperationCountOnWorkCenter=Product($numberOfOperationsOnSameResource);
alpha= Activity($hasRequiredCapacity(Product($hasActivities)));
WorkCenterCapacity= FE_Aligner($hasAvailableCapacity, $hasLocation), FE_Stepper_2($hasAvailableCapacity, $hasLocation), FE_Stepper_1($hasAvailableCapacity, $hasLocation), FE_Implanter($hasAvailableCapacity, $hasLocation);</sc3:formula>
    </NamedIndividual>
    


