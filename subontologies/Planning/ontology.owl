        <hasPlanningObjects rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#FE_Backlog"/>
        <hasPlanningObjects rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#FE_Inventory"/>
        <hasPlanningObjects rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#FE_Wip"/>
        <hasPlanningObjects rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#FE_X"/>
        <hasPlanningObjects rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#FE_Y"/>
        <hasCosts>backlog=150</hasCosts>
        <hasCosts>inventory=15</hasCosts>
        <hasCosts>wip=50</hasCosts>
        <hasPlanningWindow>12 weeks</hasPlanningWindow>
        <sc3:formula>sum($g in Products, $t in PlanningHorizon)--&gt; sum($j in Frontends) (WipCosts($g, $t) * FE_Wip($j, $g, $t)) + InventoryCosts($g, $t) * FE_Inventory($g, $t) + BacklogCosts($g, $t) * FE_Backlog($g, $t);

Frontends= Frontend_1, Frontend_2;
Products= Product_1;
InventoryCosts= $(hasCosts($inventory));
BacklogCosts= $(hasCosts($backlog));
WipCosts= $(hasCosts($wip));</sc3:formula>
    </owl:NamedIndividual>
    


