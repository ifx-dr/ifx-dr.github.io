    <!-- http://www.w3.org/2006/time#weeks -->


    <AnnotationProperty rdf:about="http://www.w3.org/2006/time#weeks"/>
    


    <!-- http://www.w3id.org/ecsel-dr-Planning#has_costs -->


    <AnnotationProperty rdf:about="http://www.w3id.org/ecsel-dr-Planning#has_costs"/>
    


