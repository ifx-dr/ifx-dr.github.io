    rdfs:domain :Event ;
    rdfs:range :Join_Rule ;
    ns1:altLabel "evalu" .

:is_executed_by_function a owl:ObjectProperty ;
    rdfs:label "is exectued by function"@en ;
    rdfs:comment "Software is executed by a function in a process flow." ;
    rdfs:domain <http://purl.org/dc/dcmitype/Software> ;
    rdfs:range :Function ;
    ns1:altLabel "exectu" .

:is_input_for a owl:ObjectProperty ;
    rdfs:label "is input for"@en ;
