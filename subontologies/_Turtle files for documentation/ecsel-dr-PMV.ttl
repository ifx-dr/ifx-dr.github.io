    rdfs:domain :Technical_Term ;
    rdfs:range :Task ;
    ns1:altLabel "input" .

:is_output_of a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "is output of"@en ;
    rdfs:comment "A software independent business object may be output of a process step.",
        "Physical or non-physical output is produced by a function in a process flow." ;
    rdfs:domain :Output,
        :Technical_Term ;
    rdfs:range :Function,
        :Task ;
    ns1:altLabel "output" .

:is_predecessor_of a owl:ObjectProperty ;
