:transforms a owl:ObjectProperty ;
    rdfs:label "transforms"@en ;
    rdfs:comment "A function in a process flow transforms environmental data like work schedules." ;
    rdfs:domain :Function ;
    rdfs:range :Environmental_Data ;
    ns1:altLabel "transform" .

:uses a owl:ObjectProperty ;
    rdfs:label "uses"@en ;
    rdfs:comment "A function in a process flow uses external resources like machines or computer hardware during its execution." ;
    rdfs:domain :Function ;
    rdfs:range [ ] ;
    ns1:altLabel "use" .

:Business_Process_Activity a owl:Class ;
    rdfs:label "Business Process Activity"@en ;
    rdfs:comment "A Business Process (BP) Activity is a part of a Business Process containing one or more sub Business Processes or a Process Flow." ;
    rdfs:subClassOf :Function ;
    ns1:altLabel "BP Activity",
        "busi process activ" .

:Environmental_Data a owl:Class ;
    rdfs:label "Environmental Data"@en ;
    rdfs:comment "Additional information to a process like work schedule, production plans, etc."@en ;
    rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/informationrealization.owl#Information_Object> ;
    ns1:altLabel "environment data" .

:Human_Output a owl:Class ;
    rdfs:label "Human Output"@en ;
    rdfs:comment "Human being that puts effort in the processing of a process step (function). It belongs to the organizational unit that is responsible for the specific process step." ;
    rdfs:subClassOf <http://xmlns.com/foaf/0.1/Person> ;
    owl:equivalentClass :Role ;
    ns1:altLabel "Role",
        "human output" .

:Input a owl:Class ;
    rdfs:label "Input"@en ;
    rdfs:comment "Item external to a system (such as a process) that is transformed by the system (usually together with one or more other items) to become an output." ;
    rdfs:isDefinedBy "http://www.businessdictionary.com/definition/input.html" ;
    rdfs:subClassOf <http://www.w3id.org/ecsel-dr#Process_Lobe> ;
    ns1:altLabel "input" .

:Join_Rule a owl:Class ;
    rdfs:label "Join Rule"@en ;
    rdfs:comment "Rule element that unites the control flow after being split into several branches. Has several incoming paths (at least two) and one outgoing." ;
    rdfs:subClassOf [ ],
        [ ] ;
    owl:disjointWith :Split_Rule ;
    ns1:altLabel "join rule" .

:activates a owl:ObjectProperty ;
    rdfs:label "activates"@en ;
    rdfs:comment "The connector is used to represent the chronological order in an EPC, where an Event or a Rule activate a Task or Process Interface. " ;
    rdfs:domain [ ] ;
    rdfs:range :Modelling_Function_Object ;
    owl:inverseOf :is_activated_by ;
    ns1:altLabel "activ" .

:leads_to a owl:ObjectProperty ;
    rdfs:label "leads to"@en ;
    rdfs:comment "The connector is used to represent the chronological order in an EPC." ;
    ns1:altLabel "lead" .

:Application_System_Type a owl:Class ;
    rdfs:label "Application System Type"@en ;
    rdfs:comment "An application system type classifies individual application systems which have exactly the same technological properties. Process steps might be supported by application systems. Object describe IT systems (library for IT systems available)." ;
    rdfs:isDefinedBy "https://wiki.scn.sap.com/wiki/pages/viewpage.action?pageId=156532992" ;
    rdfs:subClassOf <http://purl.org/dc/dcmitype/Software> ;
    ns1:altLabel "applic system type" .

:Connector a owl:Class ;
    rdfs:label "Connector"@en ;
    rdfs:comment " A connector determines the behavior of the process in case its control ?ow is split up into or joined from several execution branches." ;
    rdfs:isDefinedBy "https://www.epc-standard.org/collaborate/Operator" ;
    rdfs:subClassOf :Model_Element ;
    owl:equivalentClass :Rule ;
    ns1:altLabel "Rule",
        "connector" .

:Function_Object a owl:Class ;
    rdfs:label "Function Object"@en ;
    rdfs:comment "In IndustryPrint 5, function objects are used to model the decomposition of a process."@en ;
    rdfs:subClassOf <http://www.w3id.org/ecsel-dr#Process_Lobe> ;
    ns1:altLabel "function object" .

:Output a owl:Class ;
    rdfs:label "Output"@en ;
    rdfs:comment "The amount of energy, work, goods, or services produced by a machine, factory, company, or an individual in a period." ;
    rdfs:isDefinedBy "http://www.businessdictionary.com/definition/output.html" ;
    rdfs:subClassOf <http://www.w3id.org/ecsel-dr#Process_Lobe> ;
    ns1:altLabel "output" .

:Screen a owl:Class ;
    rdfs:label "Screen"@en ;
    rdfs:comment "A transaction in SAP is described as a screen. Here, they represent the actions undertaken in the application SAP to perform a function in a process flow." ;
    rdfs:subClassOf <http://www.w3id.org/ecsel-dr#Process_Lobe> ;
    ns1:altLabel "Transaction",
        "screen" .

:is_activated_by a owl:ObjectProperty ;
    rdfs:label "is activated by" ;
    rdfs:comment "A process step is activated by an event or a rule. The connector is used to represent the chronological order in an EPC." ;
    rdfs:domain :Modelling_Function_Object ;
    rdfs:range [ ] ;
    ns1:altLabel "activ" .

:Structural_Function_Object a owl:Class ;
    rdfs:label "Structural Function Object"@en ;
    rdfs:comment "Function objects are used to model the decomposition of the structure of a process." ;
    rdfs:subClassOf :Function_Object ;
    ns1:altLabel "structur function object" .

:Technical_Term a owl:Class ;
    rdfs:label "Technical Term"@en ;
    rdfs:comment "Set up with connection to a function or activity or VAC. It can be either input or output, where the direction of the arrow describes whether it is an input or an output. Set up of a connection is not mandatory."@en ;
    rdfs:subClassOf <http://xmlns.com/foaf/0.1/Document> ;
    ns1:altLabel "technic term" .

:Value_Added_Chain_Diagram a owl:Class ;
    rdfs:label "Value-Added Chain Diagram"@en ;
    rdfs:comment """A Value-Added Chain is a process that represents an element of the process chain. A process hierarchy can be realized by hierarchically structuring value chains, thus modeling business areas, process groups, business processes and business process variants using the sam notation.\r
House: represents enabler processes.""" ;
    rdfs:isDefinedBy "https://wiki.scn.sap.com/wiki/pages/viewpage.action?pageId=156532992" ;
    rdfs:subClassOf <http://www.w3id.org/ecsel-dr#Process_Lobe> ;
    ns1:altLabel "VACD",
        "valu - ad chain diagram" .

:Business_Process_Flow a owl:Class ;
    rdfs:label "Business Process Flow"@en ;
    rdfs:comment """An EPC describes a Business Process Flow, which is a sequence of required tasks to create specified outputs using defined inputs.\r
A Business Process Flow is a sequence of required tasks to create specified outputs using defined inputs."""@en ;
    rdfs:subClassOf <http://www.w3id.org/ecsel-dr#Process_Lobe> ;
    ns1:altLabel "Event-driven process chain"@en,
        "busi process flow"@en .

:Model_Element a owl:Class ;
    rdfs:label "Model Element"@en ;
    rdfs:comment "An EPC model element is part of the EPC meta model and can be represented by a Function, an Event, an Operator, a Process Interface.",
        "Represents an object in the EPC model. Includes all elements used in the modelling procedure of an EPC diagram.",
        "Represents process flow elements except for modelling function objects." ;
    rdfs:isDefinedBy "https://www.epc-standard.org/collaborate/Process_element" ;
    rdfs:subClassOf <http://www.w3id.org/ecsel-dr#Process_Lobe> ;
    ns1:altLabel "model element" .

:Role a owl:Class ;
    rdfs:label "Role"@en ;
    rdfs:comment "Business Role (Person) that performs task.",
        "Object to describe the role. Type of responsibility is defined by the linkage between function or activity and role. IFX options: carries out, contribute, must be informed." ;
    rdfs:subClassOf <http://xmlns.com/foaf/0.1/Person> ;
    ns1:altLabel "role" .

:Event a owl:Class ;
    rdfs:label "Event"@en ;
    rdfs:comment "An event illustrates the fact that a process object has taken on a business relevant state influencing the further procedure of a process. Unlike a function, which is a time consuming occurrence, an event is related to one point in time. Therefore, it is used to describe an event or status in the process flow. The object is mandatory for: EPC start and EPC end or after x (xor), v (or)."@en,
        "The event is a passive element since it represents a change of state but does not cause it. Furthermore it can characterize the conditions and the results of an activity, which in turn triggers the next function. An event with no incoming arc is called start event, while an event with no outgoing arc is called end event."@en ;
    rdfs:isDefinedBy "https://wiki.scn.sap.com/wiki/pages/viewpage.action?pageId=156532993"@en,
        "https://www.epc-standard.org/collaborate/Event"@en ;
    rdfs:subClassOf :Model_Element ;
    ns1:altLabel "Status"@en,
        "event"@en .

:Model a owl:Class ;
    rdfs:label "Model"@en ;
    rdfs:comment "One striking commonality between EPC meta models is the Process. One process may contain an unlimited number of elements, but a process element is always linked to a specific process. If a process contains an interface, it is always linked to one specific process and represents a refinement of a normal function.",
        "To graphically depict functional and process decomposition, function objects are diagramed on models in IndustryPrint 5." ;
    rdfs:isDefinedBy "https://www.epc-standard.org/collaborate/Process" ;
    rdfs:subClassOf <http://www.w3id.org/ecsel-dr#Process_Lobe> ;
    ns1:altLabel "model" .

:Rule a owl:Class ;
    rdfs:label "Rule"@en ;
    rdfs:comment "Rules allow to model alternative, exclusive alternative and parallel process flows." ;
    rdfs:isDefinedBy "https://wiki.scn.sap.com/wiki/pages/viewpage.action?pageId=156532993" ;
    rdfs:subClassOf :Model_Element ;
    ns1:altLabel "Connector",
        "rule" .

:Modelling_Function_Object a owl:Class ;
    rdfs:label "Modelling Function Object"@en ;
    rdfs:comment "Function objects are used to model the decomposition of the process flow."@en ;
    rdfs:subClassOf :Function_Object ;
    ns1:altLabel "model function object" .

:Function a owl:Class ;
    rdfs:label "Function"@en ;
    rdfs:comment "The function type represents several action elements as task, process interfaces or business process activities.",
        "They are the active elements of an EPC that represent the activities of a modelled process and again raise events upon completion. The extended event driven process chain expands the semantic of functions, since they may be associated with the organizational unit performing the function or the data needed and created as output by the function." ;
    rdfs:isDefinedBy "https://wiki.scn.sap.com/wiki/pages/viewpage.action?pageId=156532993",
        "https://www.epc-standard.org/collaborate/Function" ;
    rdfs:subClassOf :Model_Element ;
    owl:equivalentClass :Function_Object ;
    ns1:altLabel "function" .

:Task a owl:Class ;
    rdfs:label "Task"@en ;
    rdfs:comment "A Task is an instruction of a work package to create the output."@en,
        "The lowest level of detail, a task are the activities required to execute a subprocess. A single task within a subprocess can be manual or system supported. E.g. Create Purchase Order"@en ;
    rdfs:subClassOf :Function,
        :Model_Element,
        :Modelling_Function_Object ;
    ns1:altLabel "Process step",
        "task" .

