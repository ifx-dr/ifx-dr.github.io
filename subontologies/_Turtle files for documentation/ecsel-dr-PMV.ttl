    rdfs:domain :Business_Process_Flow ;
    rdfs:range :Task ;
    ns1:altLabel "sub flow" .

:is_under_responsibility_of a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "is under responsibility of"@en ;
    rdfs:comment "The execution of a function in a process flow is under the responsibility of an organizational unit." ;
    rdfs:domain :Function ;
    rdfs:range <http://www.w3.org/ns/org#Organizational_Unit> ;
    ns1:altLabel "respons" .

:is_worked_on_by_collaboration_of a owl:ObjectProperty ;
