    rdfs:comment "A process step is worked on by collaboration of role(s)." ;
    rdfs:domain :Task ;
    rdfs:range :Role ;
    rdfs:subPropertyOf <http://www.w3id.org/ecsel-dr-SO#is_workshop_of> ;
    ns1:altLabel "work collabor" .

:leads_to_event a owl:ObjectProperty ;
    rdfs:label "leads to event"@en ;
