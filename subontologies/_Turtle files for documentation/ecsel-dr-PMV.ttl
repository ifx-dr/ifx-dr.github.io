    rdfs:domain :Rule ;
    rdfs:range :Event ;
    rdfs:subPropertyOf :leads_to ;
    ns1:altLabel "lead event" .

:leads_to_rule a owl:ObjectProperty ;
    rdfs:label "leads to rule"@en ;
    rdfs:comment "A process step leads to a rule. The connector is used to represent the chronological order in an EPC." ;
    rdfs:domain :Modelling_Function_Object,
        :Task ;
    rdfs:range :Rule ;
    rdfs:subPropertyOf :leads_to ;
    ns1:altLabel "lead rule" .

:links a owl:ObjectProperty ;
    rdfs:label "links"@en ;
    rdfs:domain :Rule ;
    rdfs:range :Rule ;
    ns1:altLabel "link" .

:processes_function a owl:FunctionalProperty,
        owl:InverseFunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "processes" ;
    rdfs:comment "Some person, referred to as human output here, processes a function in a process flow. " ;
    rdfs:domain :Human_Output ;
    rdfs:range :Function ;
    ns1:altLabel "process" .

:relevant_to a owl:ObjectProperty ;
    rdfs:label "relevant to"@en ;
    rdfs:comment "An object is relevant for EPCs and value-added chain diagrams." ;
    rdfs:domain [ ] ;
    rdfs:range [ ] ;
    ns1:altLabel "relev" .

:represents a owl:ObjectProperty ;
    rdfs:label "represents"@en ;
    rdfs:comment "A aris:BPActivity is the graphical representation of a aris:VACD or aris:BPFlow in a aris:VACD." ;
    rdfs:domain :Business_Process_Activity,
        :Screen ;
    rdfs:range [ ],
        :Task ;
    ns1:altLabel "repres" .

:results_forwarded_to a owl:ObjectProperty ;
    rdfs:label "resultIs forwarded to"@en ;
    rdfs:comment "A result of a process step is forwarded to role(s)." ;
    rdfs:domain :Task ;
    rdfs:range :Role ;
    ns1:altLabel "result forward" .

:supports_task a owl:ObjectProperty ;
