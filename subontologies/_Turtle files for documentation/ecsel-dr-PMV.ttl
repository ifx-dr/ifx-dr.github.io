    rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Goal> ;
    ns1:altLabel "control" .

:is_created_by a owl:ObjectProperty ;
    rdfs:label "is created by"@en ;
    rdfs:comment "An event is created by a process step. The connector is used to represent the chronological order in an EPC." ;
    rdfs:domain :Event ;
    rdfs:range [ ],
        :Modelling_Function_Object ;
    ns1:altLabel "creat" .

:is_evaluated_by a owl:ObjectProperty ;
    rdfs:label "is evaluated by"@en ;
