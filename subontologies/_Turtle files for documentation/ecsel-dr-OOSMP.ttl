:Application_Level_2 a owl:Class ;
    rdfs:label "Application Level 2"@en ;
    rdfs:comment "The medium application level which can have several sub application levels (application level 3) and can be assigned to super application levels (application level 1)"@en ;
    rdfs:isDefinedBy :InfineonTechnologiesAG_Organization ;
    rdfs:subClassOf :Application_Area ;
    ns1:prefLabel "Application Level 2"@en .

:Application_Level_3 a owl:Class ;
    rdfs:label "Application Level 3"@en ;
    rdfs:comment "The lowest application level which can be assigned to super application levels (application level 2)"@en ;
    rdfs:isDefinedBy :InfineonTechnologiesAG_Organization ;
    rdfs:subClassOf :Application_Area ;
    ns1:prefLabel "Application Level 3"@en .

:Business_Line a owl:Class ;
    rdfs:comment "A business line is an organizational unit that can be assigned to a division and can have one or more product lines."@en ;
    rdfs:subClassOf <http://www.w3.org/ns/org#Organizational_Unit> ;
    ns1:prefLabel "Business Line"@en ;
    :abbreviation "BL"@en .

:Division a owl:Class ;
    rdfs:label "Division"@en ;
    rdfs:comment "A division is a administrative business unit that is smaller than the whole company, but it is self-contained and most often responsible for its own revenue, cost, and profit. It has one or more business lines."@en ;
    rdfs:subClassOf <http://www.w3.org/ns/org#Organizational_Unit> ;
    owl:equivalentClass <http://www.w3id.org/ecsel-dr-ORG#Division> ;
    ns1:altLabel "Lead Division"@en ;
    ns1:prefLabel "Division"@en ;
    :abbreviation "DIV"@en .

:Magnetic_Position_Sensor a owl:Class ;
    rdfs:label "Magnetic Position Sensor"@en ;
    rdfs:comment "A magnetic position sensor is a sensor which measures magnetic positions."@en ;
    rdfs:subClassOf :Sensor ;
    ns1:prefLabel "Magnetic Position Sensor"@en .

:Main_Application a owl:Class ;
    rdfs:label "Main Application"@en ;
    rdfs:comment "An application area on medium level (corresponing to application level 2) which can have several sub application levels (sub application) and can be assigned to super application levels (industry)"@en ;
    rdfs:isDefinedBy :InfineonTechnologiesAG_Organization ;
    rdfs:subClassOf :Application_Area ;
    ns1:prefLabel "Main Application"@en .

:Product_Line a owl:Class ;
    rdfs:label "Product Line"@en ;
    rdfs:comment "A product line is an organizational unit that can be assigned to a business line and directly owns/is responsible for products."@en ;
    rdfs:subClassOf <http://www.w3.org/ns/org#Organizational_Unit> ;
    owl:equivalentClass <http://www.w3id.org/ecsel-dr-SO#Product_Line> ;
    ns1:altLabel "Product Line"@en ;
    :abbreviation "PL"@en .

:Solution a owl:Class ;
    rdfs:label "Solution"@en ;
    rdfs:comment "A solution is a combination of products which is produced or assembled by an organization. Further, a solution is sold to a customer and solves a specific problem / fulfills a specific need."@en ;
    rdfs:subClassOf <http://www.w3id.org/ecsel-dr#Product_Lobe> ;
    ns1:prefLabel "Solution"@en .

:has_semiconductor_product_status a owl:ObjectProperty ;
    rdfs:label "has semiconductor product status"@en ;
    rdfs:domain :Semiconductor_Product ;
    rdfs:range :Semiconductor_Product_Status ;
    owl:inverseOf :is_semiconductor_product_status_of .

:chip_count a owl:DatatypeProperty ;
    rdfs:label "chip count"@en ;
    rdfs:domain :Semiconductor_Solution ;
    rdfs:range xsd:integer .

:footprint_area_product_family a owl:DatatypeProperty ;
    rdfs:label "footprint area product family"@en ;
    rdfs:domain :Semiconductor_Solution ;
    rdfs:range xsd:double ;
    rdfs:subPropertyOf :footprint_area .

:has_sub_application_area a owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "has sub application area"@en ;
    rdfs:domain :Application_Area ;
    rdfs:range :Application_Area ;
    owl:inverseOf :is_sub_application_area_of .

:is_semiconductor_product_status_of a owl:ObjectProperty ;
    rdfs:label "is semiconductor product status of"@en ;
    rdfs:domain :Semiconductor_Product_Status ;
    rdfs:range :Semiconductor_Product .

:Qualification a owl:Class ;
    rdfs:label "Qualification"@en ;
    rdfs:comment "If a product/solution has a certain qualification, the product/solution fulfills the standards of this certain industry."@en ;
    rdfs:subClassOf <http://www.w3id.org/ecsel-dr#Product_Lobe> ;
    owl:equivalentClass [ ] ;
    ns1:prefLabel "Qualification"@en .

:Semiconductor_Product_Status a owl:Class ;
    rdfs:label "Semiconductor Product Status"@en ;
    rdfs:comment "A semiconductor product can have different product states which describe the product characteristics more closely."@en ;
    rdfs:subClassOf <http://www.w3id.org/ecsel-dr#Product_Lobe> ;
    ns1:prefLabel "Semiconductor Product Status"@en .

:is_sub_application_area_of a owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "is sub application area of"@en ;
    rdfs:domain :Application_Area ;
    rdfs:range :Application_Area .

:contains_product a owl:ObjectProperty ;
    rdfs:label "contains product"@en ;
    rdfs:domain :Solution ;
    rdfs:range :Product ;
    owl:inverseOf :is_part_of .

:is_part_of a owl:ObjectProperty ;
    rdfs:comment "is part of"@en ;
    rdfs:domain :Product ;
    rdfs:range :Solution .

:Product a owl:Class ;
    rdfs:label "Product"@en ;
    rdfs:comment "A product is something that is produced with effort (e.g. by mechanical or industrial processes) and sold to a customer. A product is either tangible (physical product) or intangible (service)."@en,
        "A solution is a combination of products which is sold to a customer. Thereby, a solution solves a problem or fulfills a customer need."@en ;
    rdfs:subClassOf <http://www.w3id.org/ecsel-dr#Product_Lobe> ;
    owl:equivalentClass <http://www.w3id.org/ecsel-dr-PROD#Product>,
        <http://www.w3id.org/ecsel-dr-PWR#Product>,
        <http://www.w3id.org/ecsel-dr-SO#Product> ;
    ns1:prefLabel "Product"@en .

:Sensor a owl:Class ;
    rdfs:label "Sensor"@en ;
    rdfs:comment "A semiconductor product which is used as sensor"@en ;
    rdfs:subClassOf :Semiconductor_Product ;
    owl:equivalentClass <http://www.w3id.org/ecsel-dr-RAMI40#Sensor> ;
    ns1:prefLabel "Sensor"@en .

:Application_Area a owl:Class ;
    rdfs:label "Application Area"@en ;
    rdfs:comment "A way in which something can be used for a particular purpose."@en ;
    rdfs:subClassOf <http://www.w3id.org/ecsel-dr#Application> ;
    owl:equivalentClass <http://www.w3id.org/ecsel-dr-PWR#Application_Area> ;
    ns1:prefLabel "Application Area"@en .

:Semiconductor_Solution a owl:Class ;
    rdfs:label "Semiconductor Solution"@en ;
    rdfs:comment "A semiconductor solution is combination of semiconductor products and is sold within the seminconductor industry."@en ;
    rdfs:subClassOf :Solution ;
    ns1:prefLabel "Semiconductor Solution"@en .

:Semiconductor_Product a owl:Class ;
    rdfs:label "Semiconductor Product"@en ;
    rdfs:comment "A semiconductor product is a physical product that is produced and sold within the semiconductor industry."@en ;
    rdfs:subClassOf :Physical_Product ;
    ns1:prefLabel "Semiconductor Product"@en .

:Motor_Control_And_Drive_Solution a owl:Class ;
    rdfs:label "Motor Control And Drive Solution"@en ;
    rdfs:comment "A motor control and drive solution is a semiconductor solution which function is to drive and control a motor."@en ;
    rdfs:subClassOf :Semiconductor_Solution ;
    ns1:prefLabel "Motor Control And Drive Solution"@en .

